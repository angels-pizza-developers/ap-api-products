{
  "name": "ap-api-products",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:typeorm:models": "node generate-models.js",
    "dev:typeorm:repositories": "node generate-repositories.js",
    "dev:generate:typeorm": "npm run dev:typeorm:models && npm run dev:typeorm:repositories",
    "migration:generate": "npx typeorm migration:generate -d ./data-source.ts -n GeneratedMigration",
    "migration:run": "npx typeorm migration:run -d ./data-source.ts",
    "migration:revert": "npx typeorm migration:revert -d ./data-source.ts"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.670.0",
    "@firebase/auth": "^1.7.9",
    "@firebase/storage": "^0.13.2",
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "firebase-admin": "^12.5.0",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "http-proxy-middleware": "^3.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "nestjs-pusher": "^2.1.0",
    "nestjs-typeorm-paginate": "^4.0.4",
    "node-base64-to-file": "^1.0.4",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.0",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "stream-buffers": "^3.0.3",
    "swagger-ui-express": "^5.0.1",
    "ts-morph": "^24.0.0",
    "typeorm": "^0.3.20",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
